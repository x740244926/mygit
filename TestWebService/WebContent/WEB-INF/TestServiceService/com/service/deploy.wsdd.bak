<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestServiceService WSDL service -->

  <service name="TestService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.com"/>
      <parameter name="wsdlServiceElement" value="TestServiceService"/>
      <parameter name="schemaQualified" value="http://service.com"/>
      <parameter name="wsdlServicePort" value="TestService"/>
      <parameter name="className" value="com.service.TestServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TestService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="checkUserRight" qname="operNS:checkUserRight" xmlns:operNS="http://service.com" returnQName="retNS:checkUserRightReturn" xmlns:retNS="http://service.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userId" xmlns:pns="http://service.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://service.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="checkUserRight"/>

  </service>
</deployment>
